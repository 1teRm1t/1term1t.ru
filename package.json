{
  "name": "start-template",
  "version": "1.0.0",
  "description": "Шаблон сборки проектов",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "10.0.*",
    "browser-sync": "2.26.*",
    "del": "6.0.*",
    "eslint": "7.10.*",
    "eslint-config-htmlacademy": "0.6.*",
    "gulp": "4.0.*",
    "gulp-concat": "^2.6.1",
    "gulp-csso": "4.0.*",
    "gulp-htmlmin": "5.0.*",
    "gulp-imagemin": "7.1.*",
    "gulp-plumber": "1.2.*",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "2.0.*",
    "gulp-resolve-url": "0.0.2",
    "gulp-sass": "4.1.*",
    "gulp-sourcemaps": "2.6.*",
    "gulp-svgstore": "^7.0.1",
    "gulp-uglify-es": "2.0.*",
    "gulp-webp": "^4.0.1",
    "normalize-scss": "7.0.*",
    "postcss": "^8.1.4",
    "stylelint": "13.*.*",
    "stylelint-config-htmlacademy": "0.1.*"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "gulp"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.scss"
  ],
  "engines": {
    "node": "12.18.3"
  },
  "dependencies": {
    "normalize-scss": "^7.0.1"
  }
}
